{
  "rules": [
    {
      "match": {
        "ACTION" : ["add", "remove"],
        "DEVNAME": [
            "hd*",
            "fd*",
            "ram*",
            "raw*",
            "sd*",
            "dasd*",
            "loop*",
            "md*",
            "mmc*"
        ],
        "SUBSYSTEM": "block",
        "ID_VENDOR_ID": "0781",
        "ID_MODEL_ID": "558b"
      },
      "apply": {
        "perm": "666",
        "gid": "5",
        "uid": "0"
      }
    },
    {
      "match": {
        "ACTION": ["add", "remove"],
        "DEVNAME": [
          "ttyUSB*",
          "ttyS*",
          "ttyACM*"
        ],
        "SUBSYSTEM": "tty",
        "ID_VENDOR_ID": "0403",
        "ID_MODEL_ID": "6001"
      },
      "apply": {
        "perm": "644",
        "gid": "11",
        "uid": "0"
      }
    }
  ]
}